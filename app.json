{
  "expo": {
    "name": "AllInOne",
    "slug": "AllInOne",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "com.eduardotlopes.allinone",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/CooperHewitt-Bold.otf",
            "./assets/fonts/CooperHewitt-BoldItalic.otf",
            "./assets/fonts/CooperHewitt-Book.otf",
            "./assets/fonts/CooperHewitt-BookItalic.otf",
            "./assets/fonts/CooperHewitt-Heavy.otf",
            "./assets/fonts/CooperHewitt-HeavyItalic.otf",
            "./assets/fonts/CooperHewitt-Light.otf",
            "./assets/fonts/CooperHewitt-LightItalic.otf",
            "./assets/fonts/CooperHewitt-Medium.otf",
            "./assets/fonts/CooperHewitt-MediumItalic.otf",
            "./assets/fonts/CooperHewitt-Semibold.otf",
            "./assets/fonts/CooperHewitt-SemiboldItalic.otf",
            "./assets/fonts/CooperHewitt-Thin.otf",
            "./assets/fonts/CooperHewitt-ThinItalic.otf"
          ]
        }
      ],
      "expo-router"
    ]
  }
}
